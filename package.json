{
    "name": "proxy-server",
    "version": "0.0.2",
    "private": true,
    "module": "src/index.ts",
    "main": "lib/index.js",
    "type": "module",
    "scripts": {
        "build": "bun build ./src/index.ts --outdir ./lib --target node",
        "start": "bun run src/index.ts",
        "check-types": "tsc --noEmit",
        "convert": "bun run src/utils/convert.ts",
        "test-proxies": "bun run src/utils/ping.ts",
        "test": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' mocha",
        "test:watch": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' mocha --watch",
        "test:coverage": "c8 mocha"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/chai": "^5.0.1",
        "@types/mocha": "^10.0.10",
        "@types/nock": "^11.1.0",
        "@types/node": "^20.0.0",
        "@types/sinon": "^17.0.4",
        "c8": "^10.1.3",
        "chai": "^5.2.0",
        "mocha": "^11.1.0",
        "nock": "^14.0.1",
        "sinon": "^19.0.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "https-proxy-agent": "^7.0.6",
        "node-fetch": "^3.3.2"
    }
}