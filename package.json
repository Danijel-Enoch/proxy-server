{
    "name": "proxy-server",
    "version": "0.0.2",
    "private": true,
    "module": "src/index.ts",
    "main": "lib/index.js",
    "type": "module",
    "scripts": {
        "build": "bun build ./src/index.ts --outdir ./lib --target node",
        "start": "bun run src/index.ts",
        "check-types": "tsc --noEmit",
        "convert": "bun run src/utils/convert.ts",
        "test-proxies": "bun run src/utils/ping.ts"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "https-proxy-agent": "^7.0.6",
        "node-fetch": "^3.3.2"
    }
}